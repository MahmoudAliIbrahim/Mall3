<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <View android:id="@+id/myorder_details_maintenance_coupon_line"  style="@style/DotLine"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/myorder_details_maintenance_coupon_preferentialDescription"
            style="@style/BlackText14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="已使用的优惠卷"/>
        <TextView
            android:id="@+id/myorder_details_maintenance_coupon_price"
            style="@style/PriceText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="抵扣 ¥ 10.00"/>
    </LinearLayout>
</LinearLayout>
